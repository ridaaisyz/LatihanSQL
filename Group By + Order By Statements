SELECT Gender
FROM Latihasql.dbo.EmployeeDemographics
GROUP BY Gender

SELECT Gender, COUNT(Gender)
FROM Latihasql.dbo.EmployeeDemographics
GROUP BY Gender

SELECT Gender, age, COUNT(Gender)
FROM Latihasql.dbo.EmployeeDemographics
GROUP BY Gender, Age

SELECT Gender, COUNT(Gender)
FROM Latihasql.dbo.EmployeeDemographics
WHERE Age > 30
GROUP BY Gender

SELECT Gender, COUNT(Gender) AS CountGender
FROM Latihasql.dbo.EmployeeDemographics
WHERE Age > 30
GROUP BY Gender
ORDER BY CountGender

SELECT Gender, COUNT(Gender) AS CountGender
FROM Latihasql.dbo.EmployeeDemographics
WHERE Age > 30
GROUP BY Gender
ORDER BY CountGender DESC

SELECT Gender, COUNT(Gender) AS CountGender
FROM Latihasql.dbo.EmployeeDemographics
WHERE Age > 30
GROUP BY Gender
ORDER BY CountGender ASC

SELECT *
FROM Latihasql.dbo.EmployeeDemographics
ORDER BY Age

SELECT *
FROM Latihasql.dbo.EmployeeDemographics
ORDER BY Age ASC

SELECT *
FROM Latihasql.dbo.EmployeeDemographics
ORDER BY Age DESC

SELECT *
FROM Latihasql.dbo.EmployeeDemographics
ORDER BY Age, Gender

SELECT *
FROM Latihasql.dbo.EmployeeDemographics
ORDER BY 4, 5 ASC
