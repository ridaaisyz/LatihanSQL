--Company whant to update plankton data by adding hourly pay and hire date--
        UPDATE employees
        SET hire_date = '2023-06-07',
            hourly_pay = '10.25'
        WHERE employee_id = 6;

UPDATE employeedemographics
SET emp_id = 1012
WHERE firstname = 'Holly' AND lastname = 'Flax'

UPDATE employeedemographics
